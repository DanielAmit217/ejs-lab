<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= category %></title>
</head>
<body>
  <h1><%= category %></h1>
  <% menuItems.forEach((item) => { %>
        <p><strong><%= item.name %></strong> $ <%= item.price  %></p> 
        <% if (item.rating === 5) { %>
            <p>Rating: <%= item.rating %> ‚≠ê</p>
        <% } else { %>
            <p>Rating: <%= item.rating %></p>
       <% } %>
       <p><%= item.details %></p>
    <% }) %>
</body>
</html>
